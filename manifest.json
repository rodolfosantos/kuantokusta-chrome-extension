{
   "background": {
      "page": "background.htm",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "img/extension_icon.png",
      "default_popup": "popup.htm",
      "default_title": "KuantoKusta Price Alert"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "KuantoKusta Price Alert for www.kuantokusta.pt",
   "icons": {
      "128": "img/extension_icon.png",
      "16": "img/extension_icon.png",
      "32": "img/extension_icon.png",
      "48": "img/extension_icon.png"
   },
   "manifest_version": 2,
   "name": "KuantoKusta Price Alert",
   "options_page": "options.htm",
   "permissions": [ "tabs", "*://*/", "chrome://favicon/", "unlimitedStorage", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
